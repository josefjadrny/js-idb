{
  "name": "js-idb",
  "version": "1.0.1",
  "description": "Simple JSON database with fast search over data via indexes and type checking",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "tsc --noEmit && eslint .",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "prepublishOnly": "npm run build",
    "preversion": "npm run lint && npm test && npm run build",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "keywords": [
    "database",
    "json",
    "json-database",
    "indexed",
    "index",
    "search",
    "schema",
    "validation",
    "typescript",
    "node",
    "browser",
    "esm",
    "no-dependencies",
    "embedded-database",
    "file-persistence",
    "in-memory"
  ],
  "author": "bc.josefjadrny@gmail.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josefjadrny/js-idb.git"
  },
  "homepage": "https://github.com/josefjadrny/js-idb#readme",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.13",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  }
}
